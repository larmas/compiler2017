/* RETURN TEST */

boolean returnBooleanExpr(){
    boolean x;
    x = true;
    return x && false;
}

int returnIntExpr(){
    return 1+2;
}

boolean returnBooleanLiteral(){
    return true;
}

boolean returnBooleanVar(){
    boolean t;
    t = false;
    return t;
}

int returnIntegerLit(){
    return 10;
}

int returnIntegerVar(){
    int x;
    x = 10;
    return x;
}

void main(){
    returnBooleanExpr();
    returnIntExpr();
    returnBooleanLiteral();
    returnBooleanVar();
    returnIntegerLit();
    returnIntegerVar();
    return ;
}
