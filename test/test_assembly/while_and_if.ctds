/*
    WHILE AND IF SENTENCE TEST

    Salida:
        30
        10

 */

int incr(){
    int x;
    x = 1;
    while(x<20){
        x = x + 1;
        while(x < 30){
            x = x + 1;
        }
    }
    return x;
}

int hastaIgual(){
    int x;
    x = 14;
    while (! (x == 10) ){
        if( x > 10){
            x = x - 1;
        }else{
            if (x < 10){
                x = x + 1;
            }
        }
    }
    return x;
}

void main(){
    incr();
    hastaIgual();
    return ;
}
